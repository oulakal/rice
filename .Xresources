!SETTINGS
URxvt*font:     xft:Meslo LG S:pixelsize=14

*background: #171b33
*foreground: #e8e8e8
!
! ! black
*color0:     #ba88a8
*color8:     #ba88a8
!
! ! red
*color1:     #b77ba8
*color9:     #b77ba8
!
! ! green
*color2:     #8163a3
*color10:    #8163a3
!
! ! yellow
*color3:     #655a9e
*color11:    #655a9e
!
! ! blue
*color4:     #234d9b
*color12:    #234d9b
!
! ! magenta
*color5:     #4f4276
*color13:    #4f4276
!
! ! cyan
*color6:     #8d4366
*color14:    #8d4366
!
! ! white
*color7:     #b14b89
*color15:    #b14b89
!
! !cursor
*cursorColor: #e8e8e8
!URXVT SETTINGS
!URxvt*dynamicColors: on
URxvt.internalBorder: 15
URxvt*lineSpace: 2
URxvt*scrollBar: false
URxvt*scrollColor: #e8e8e8
URxvt.geometry: 80x18
URxvt*saveLines: 1000
URxvt*cursorUnderline: false
URxvt*cursorColor: #e8e8e8
URxvt*cursorBlink: true
URxvt*underlineColor: #e8e8e8
URxvt*allow_bold: true

!PERL
URxvt*perl-lib: /usr/lib/urxvt/perl/
URxvt*perl-ext-common : default,matcher

!CLICKABLE LINKS
URxvt*url-launcher: qutebrowser
URxvt*matcher.button: 1

!MISC
Xcursor*theme: 		pixelfun
Xcursor*size:  		16
XTerm*scrollBar: 	false
XTerm*rightScrollBar: 	false
XTerm*dynamicColors: true
XLogo*geometry: 	150x150
Xclock*geometry: 	150x150-0-0
Xclock*background: #121314
